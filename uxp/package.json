{
  "name": "com.adobe.example.desktop-helper-plugin",
  "version": "1.0.0",
  "license": "none",
  "private": true,
  "scripts": {
    "watch": "nodemon -w src -e js,jsx,json,css,html -w webpack.config.js -x npm run build",
    "build": "webpack --mode development",
    "uxp:load": "cd dist && uxp plugin load",
    "uxp:reload": "cd dist && uxp plugin reload",
    "uxp:watch": "cd dist && nodemon --exec \"uxp plugin reload\" -e js,css,html",
    "uxp:debug": "cd dist && uxp plugin debug"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "file-loader": "^6.2.0",
    "nodemon": "^2.0.15",
    "style-loader": "^3.3.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "socket.io-client": "^4.1.2"
  },
  "resolutions": {
    "acorn": "npm:acorn-with-stage3"
  }
}
