{
  "name": "art-factory.uxp-host",
  "version": "1.0.0",
  "license": "none",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "concurrently -k \"npm:start:react\" \"npm:start:electron\"",
    "start:react": "cross-env BROWSER=none node scripts/start.js",
    "start:electron": "electron public/electron.js",
    "build": "npm run build:react && npm run build:electron",
    "build:react": "node scripts/build.js",
    "build:electron": "electron-builder"
  },
  "devDependencies": {
    "@adobe/react-spectrum": "^3.11.2",
    "autoprefixer": "^10.4.2",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "electron": "^13.1.6",
    "electron-builder": "^22.11.7",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/runtime": "^7.17.2",
    "@openzeppelin/contracts": "^4.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@react-spectrum/color": "^3.0.0-beta.8",
    "@spectrum-css/fieldlabel": "^4.0.13",
    "@svgr/webpack": "^5.5.0",
    "@walletconnect/web3-provider": "^1.7.1",
    "ag-psd": "^14.3.5",
    "assert": "^2.0.0",
    "axios": "^0.26.0",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "buffer": "^6.0.3",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.1",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "ethers": "^5.5.4",
    "express": "^4.17.1",
    "fabric": "^5.2.1",
    "fabricjs-react": "^1.0.8",
    "file-loader": "^6.2.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "https-browserify": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.3",
    "jest-resolve": "^27.4.2",
    "jest-watch-typeahead": "^1.0.0",
    "jimp": "^0.16.1",
    "mini-css-extract-plugin": "^2.4.5",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.0.1",
    "prompts": "^2.4.2",
    "react": "^17.0.2",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.0",
    "react-dom": "^17.0.2",
    "react-flow-renderer": "^9.7.4",
    "react-refresh": "^0.11.0",
    "react-router-dom": "^6.2.1",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.3.0",
    "semver": "^7.3.5",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.3",
    "solc": "^0.8.11",
    "source-map-loader": "^3.0.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.5",
    "url": "^0.11.0",
    "uuid": "^8.3.2",
    "webpack": "^5.64.4",
    "webpack-dev-server": "^4.6.0",
    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "build": {
    "appId": "com.electron.adobe.example.desktop-helper-sample",
    "productName": "UXP Helper App",
    "files": [
      "build/**/*",
      "contracts/**/*",
      "node_modules/**/*",
      "public/**/*",
      ".env",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "asar": false,
      "target": [
        "nsis"
      ]
    },
    "directories": {
      "output": "dist"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
